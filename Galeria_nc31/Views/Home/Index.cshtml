@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Administración de centros de cómputo</h1>
    <h4>Segundo Examen Parcial</h4>
    <h4>Carné: 2017PS651 Nombre: Gerardo Vladimir Paniagua Sandoval</h4>
</div>

<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel" data-interval="3000">
    <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src="https://2017ps651p2.blob.core.windows.net/gallery/clouds-1837129_1920.jpg" width="850" height="480" alt="Imagen 1">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="https://2017ps651p2.blob.core.windows.net/gallery/fog-5038082_1920.jpg" width="850" height="480" alt="Imagen 2">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="https://2017ps651p2.blob.core.windows.net/gallery/mountain-1462655_1920.jpg" width="850" height="480" alt="Imagen 3">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="https://2017ps651p2.blob.core.windows.net/gallery/mountain-range-1246273_1920.jpg" width="850" height="480" alt="Imagen 4">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="https://2017ps651p2.blob.core.windows.net/gallery/mountains-919040_1920.jpg" width="850" height="480" alt="Imagen 5">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>


